  <div style="    margin: 75px;
    width: 40%;
    padding: 15px;" id="content">
    {{#if docShow}}
    <h1>List of Doctors:</h1>
    {{#each model as |doclist|}}
    <div class="col">
      <div style="border: 3px solid grey; padding:5px;" class="col">
        <div class="row-sm-3" style="background-color:lavender;">{{doclist.name}}</div>
        <div class="row-sm-3" style="background-color:lavender;">Username  :
          <span style="font-weight:600;"> {{doclist.username}}</span>
        </div>
        <div class="row-sm-3" style="background-color:lavender;">Email  :
          <span style="font-weight:600;"> {{doclist.email}}</span>
        </div>
      </div>
    </div>

    <br>
    <br>
    <br>
    <br> {{/each}} {{/if}} {{#if patientShow}}
    <p>This is pat list</p>
    {{/if}}
