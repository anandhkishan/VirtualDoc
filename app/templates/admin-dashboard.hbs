<div class="wrapper">
  <nav id="sidebar">
    <div class="sidebar-header">
      <h3>Admin Dashboard
      </h3>
    </div>
    <ul class="list-unstyled components">

      <li>
        <a {{action "DocList"}} style="color:black;" href="#">Doctors List</a>

      </li>
      <li>
        <a {{action "PatList"}} style="color:black;" href="#">Patients List</a>
      </li>

    </ul>
